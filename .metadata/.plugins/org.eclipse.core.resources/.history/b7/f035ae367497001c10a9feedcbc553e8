
public class Patterns {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//outer loop
	for(int j=0;j<=4;j++) {
		
		//inner loop
		for(int i=0;i<=5;i++) {
			System.out.print(" * ");
		}
		System.out.println();}

	
	
        int n=9;
	//for(int i=1; i<=n; i++) {
		for(int j=1; j<=9; j++) {
			    System.out.print("*");
			    }
		System.out.println();
		}
	
	
	
	
    int u=9;
	for(int i=1; i<=u; i++) {
		for(int j=9; j>=i; j--) {
			    System.out.print("*");
			    }
		System.out.println();
		}
	
	
	
    int o=9;
for(int i=o; i>=1; i--) {
	for(int j=1; j<=i; j++) {
		    System.out.print("*");
		    }
	System.out.println();
	}
	


int p=9;
for(int i=1; i<=p; i++) {
	for(int j=1;j<=i;j++) {
	System.out.print("*");
	}
    System.out.println();
	}

	
	
	
	
	
	
	
	
	
	}
	

	}
	


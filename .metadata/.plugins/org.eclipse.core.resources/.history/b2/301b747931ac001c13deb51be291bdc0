package practice;

import java.util.Scanner;

public class reverseTHEnumber {
	public static void main(String[] args) {
		
		Scanner scr=new Scanner(System.in);
		int number=scr.nextInt();
	
		int temp=number;       //657
		int rem=0;
		int sum=0;
		while(number>0) { 
			        //  7    Reminder                   //  5
			 sum=rem+(sum*10);     //0= 7 + ( 0 * 10 ) =7    sum= 7    // sum=(7*10)+5=75 
			 number=number/10;     //65    for next loop
		}
		System.out.println(" revers of "+temp +" is "+sum);
		
		
		
		
		
		
		
		
	}

}

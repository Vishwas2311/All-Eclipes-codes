package Patterns;

public class pyramid123 {
	public static void main(String[] args) {
		
	
/*
 * 1        syso(i)
 * 22
 * 333
 * 4444
 * 55555      
 * 666666
 * 7777777
 * 88888888
 * 999999999	
 */
		
		int a=9;
	
		for(int i=1;i<=a;i++) {
			for(int j=1;j<=i;j++) {
		    	System.out.print(i);
		    }
			System.out.println();
		}
	/*
	 * 1
	 * 23
	 * 456
	 * 789100
	 * 1112131415
	 * 161718192021
	 * 22232425262728
	 * 2930313233343536
	 * 373839404142434445
	 * 	
	 */
		int u=1;
		int o=9;
		for(int i=1;i<=o;i++) {
			for(int j=1;j<=i;j++) {
				//for incrementing
				
				System.out.print(u);
				u++;
			}
			System.out.println();
		}
		
/*1
 *12
 *123
 *1234
 *12345
 *123456
 *1234567
 *12345678
 *123456789
 */
		
		int m=1;
		int n=9;
		for(int i=1;i<=n;i++) {
			for(int j=1;j<=i;j++) {
				//for incrementing
				
				System.out.print(m);
				m++;
			}
			System.out.println();
			m=m*0;
			m=1;
		}
		
		
		
		
		
		
		
		
	}

}

package Quesions;

public class ReversString {
public static void main(String[] args) {
	 
	//Logic
	
	
	String p ="Velocity";
	String sum = null;
	int v= p.length();
	for(int i=v-1;i>=1;i--) {
		 sum= sum + p.charAt(i);
	}
	
	System.out.println(sum);
	
	
	
	
	
	
	
	StringBuilder k =new StringBuilder();
	k.append("Vishwas shinde");
	k.reverse();
	System.out.println(k);
	
	
	
	

}
}

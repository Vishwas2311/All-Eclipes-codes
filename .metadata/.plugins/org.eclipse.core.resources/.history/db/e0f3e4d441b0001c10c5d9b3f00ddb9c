package String1;

public class Reverss {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String a= "Automation selenium Maven TestNG";
		
		String b [] = a.split(" ");
		System.out.println(b);
		
		for(int i=a.length()-1;i>=0;i--) {	
		char b1 = a.charAt(i);
		System.out.print(b1);
		}
	}

}

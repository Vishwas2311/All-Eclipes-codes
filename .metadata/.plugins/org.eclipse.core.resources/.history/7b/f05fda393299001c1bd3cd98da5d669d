package Patterns;

public class pyramid123 {
	public static void main(String[] args) {
		
	
/*
 * 1        syso(i)
 * 22
 * 333
 * 4444
 * 55555      
 * 666666
 * 7777777
 * 88888888
 * 999999999	
 */
		
		int a=9;
	
		for(int i=1;i<=a;i++) {
			for(int j=1;j<=i;j++) {
		    	System.out.print(i);
		    }
			System.out.println();
		}
	/*
	 * 1
	 * 12
	 * 123
	 * 1234
	 * 12345
	 * 123456
	 * 1234567
	 * 12345678
	 * 123456789
	 * 	
	 */
		int u=1;
		int o=9;
		for(int i=1;i<=o;i++) {
			for(int j=1;j<=i;j++) {
				//for incrementing
				
				System.out.print( u);
				u++;
			}
			System.out.println();
		}
		
		
		
		
		
		
		
	}

}

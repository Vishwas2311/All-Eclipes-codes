package String1;

import org.testng.annotations.Test;


public class Revers {

	public static void main(String[] args) {
		
	
		
		String a= new String("Automation selenium Maven TestNG");
		
		String[] b = a.split(" ");
		System.out.println(b);
		
		for(int i=a.length()-1;i>=0;i--) {	
		char b = a.charAt(i);
		System.out.print(b);
		}
	
	
	}}

